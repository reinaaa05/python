# 初期の地図
landmap = [["森" for i in range(19)] for j in range(10)]
landmap[2][9] = "城"
landmap[0][0] = "町"
landmap[0][18] = "町"
landmap[9][0] = "町"
landmap[9][18] = "町"

# 地図に道を作る
for i, line in enumerate(landmap):
    for j, area in enumerate(line):
        # ここから足りないところを入力してください
            if (i % 9 == 0 or j % 9 == 0) and area == "森":
                print("＋", end="")
            else:
                print(area, end="")
    print()
